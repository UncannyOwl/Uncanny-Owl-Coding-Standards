{
  "name": "uocs/uncanny-owl-coding-standards",
  "require-dev": {
    "squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "*",
    "phpcompatibility/php-compatibility": "*",
    "phpcompatibility/phpcompatibility-wp": "*",
    "wpengine/wpengine-coding-standards": "*",
    "phpmd/phpmd": "*",
    "friendsofphp/php-cs-fixer": "*",
    "vimeo/psalm": "*",
    "phpstan/phpstan": "*",
    "humanmade/psalm-plugin-wordpress": "^2.0"
  },
  "prefer-stable": true,
  "scripts": {
    "post-install-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths './../../../vendor/wp-coding-standards/wpcs,./../../../vendor/wpengine/wpengine-coding-standards,./../../../vendor/phpcompatibility/php-compatibility,./../../../vendor/phpcompatibility/phpcompatibility-paragonie,./../../../vendor/phpcompatibility/phpcompatibility-wp,./../../../UO-Coding-Standards'",
    "post-update-cmd": "\"vendor/bin/phpcs\" --config-set installed_paths './../../../vendor/wp-coding-standards/wpcs,./../../../vendor/wpengine/wpengine-coding-standards,./../../../vendor/phpcompatibility/php-compatibility,./../../../vendor/phpcompatibility/phpcompatibility-paragonie,./../../../vendor/phpcompatibility/phpcompatibility-wp,./../../../UO-Coding-Standards'"
  },
  "extra": {
    "installer-paths": {
    }
  },
